{
    "manifest_version": 2,
    "name": "TV leanback interface for youtube",
    "description": "Makes youtube.com/tv still work",
    "version": "0.0.1",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*.youtube.com/tv*"
    ],
    "background": {
        "scripts": ["background.js"]
    }



}
